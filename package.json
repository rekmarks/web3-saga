    "@redux-saga/testing-utils": "^1.0.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-preset-minify": "^0.5.0",
    "eslint": "^5.13.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-json": "^1.3.2",
    "eslint-plugin-react": "^7.12.4",
